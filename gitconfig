[user]
	name = Edouard Misset
	email = edouardmisset@gmail.com
[init]
	defaultBranch = main
[core]
	editor = code --wait
[alias]
	se = !git rev-list --all | xargs git grep -F
[help]
	autocorrect = 20
[diff]
  tool = vscode
[difftool "vscode"]
  cmd = code --wait --diff $LOCAL $REMOTE
[merge]
  tool = vscode
[mergetool "vscode"]
  cmd = code --wait $MERGED
